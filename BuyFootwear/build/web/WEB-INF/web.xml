        <servlet-name>SearchServlet</servlet-name>
        <url-pattern>/Search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserLogoutSession</servlet-name>
        <url-pattern>/UserLogoutSession</url-pattern>
